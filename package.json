{
  "type": "module",
  "name": "chassis-css",
  "version": "4.0.8",
  "description": "A minimalistic grid & typography CSS framework",
  "keywords": [
    "css",
    "flexbox",
    "framework",
    "front-end",
    "mobile-first",
    "responsive",
    "sass",
    "web"
  ],
  "main": "dist/css/chassis.min.css",
  "unpkg": "dist/css/chassis.min.css",
  "style": "dist/css/chassis.min.css",
  "repository": "https://github.com/joeleisner/chassis-css",
  "homepage": "https://chassis.joeleisner.com",
  "author": "Joel Eisner <jeisner93@gmail.com>",
  "scripts": {
    "build": "NODE_ENV=production npx gulp build && NODE_ENV=development npx gulp build",
    "develop": "NODE_ENV=development npx gulp develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "gulp": "^4.0.2",
    "gulp-filter": "^7.0.0",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "postcss": "^8.3.11",
    "sass": "^1.29.0"
  },
  "files": [
    "dist",
    "changelog.md",
    "license",
    "readme.md"
  ],
  "browserslist": [
    "last 2 versions and > 0% and not dead",
    "not Explorer <= 11",
    "not ExplorerMobile 11"
